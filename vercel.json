{
    "version": 2,
    "buildCommand": "npm start && npm postinstall",
    "installCommand": "npm install",
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "HEAD", "PUT", "PATCH", "POST", "DELETE"]
      }
    ]
  }